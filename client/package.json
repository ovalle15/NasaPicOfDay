{
    "name": "client",
    "packageManager": "yarn@4.7.0",
    "scripts": {
        "safe-rm-ycache": "rm -rf .yarn/ || true",
        "safe-rm-dist": "rm -rf dist/ || true",
        "safe-rm-node-modules": "rm -rf node_modules/ || true",
        "build:ts": "tsc -b --verbose",
        "build:dev": "yarn build:ts && webpack --mode=development --node-env=development",
        "build:prod": "webpack --mode=production --node-env=production",
        "build": "yarn build:prod",
        "watch": "webpack --mode=development --node-env=development --watch",
        "clean": "yarn safe-rm-ycache && yarn safe-rm-dist && yarn safe-rm-node-modules && yarn cache clean",
        "install:clean": "yarn clean && yarn install",
        "serve": "nodemon --inspect=0.0.0.0:9229 -e js,handlebars -w src --exec src/client/entry.js",
        "start": "yarn build:dev && yarn serve",
        "restart": "yarn install:clean && yarn build:dev && yarn serve"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-handlebars": "^7.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.24.3",
        "@babel/node": "^7.23.9",
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.24.3",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^12.0.2",
        "html-webpack-plugin": "^5.6.0",
        "nodemon": "^3.0.3",
        "webpack": "^5.90.3",
        "webpack-assets-manifest": "^5.2.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.2",
        "webpack-node-externals": "^3.0.0",
        "workbox-webpack-plugin": "^7.0.0"
    }
}
